<script>

function openInfoDialog()
{
    Dialog.info("Prosim cakajte ... <br>Please wait ...", {width:250, height:100, showProgress: true});
}
    
var aForms = document.getElementsByTagName("form");

for (iii=0; iii<aForms.length; iii++)
{
    if (aForms[iii].id == 'search' || aForms[iii].id=='tx_indexedsearch')
	aForms[iii].onsubmit = openInfoDialog;
}

</script>
